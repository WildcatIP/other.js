{
  "name": "other",
  "version": "3.12.8",
  "description": "other.js - the chatternet platform",
  "author": "Post Social, inc. <root@other.xyz>",
  "repository": "other-xyz/other.js",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "webpack --progress && babel --presets es2015 -d dist lib",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "deploy": "./deploy.sh",
    "dist": "NODE_ENV=production npm run build && jsdoc ./index.js lib/*.js -c ./jsdoc/conf.json -R ./GETSTARTED.md",
    "pretest": "eslint --ignore-path .gitignore --ignore-pattern 'pseudo/*' .",
    "start": "webpack-dev-server --host 0.0.0.0 --port=8888",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "node-fetch": "^1.6.3",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-no-unsafe-regex": "^1.0.0",
    "git-revision-webpack-plugin": "^2.4.0",
    "jest-cli": "^18.1.0",
    "jsdoc": "^3.4.3",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "no-unsafe-regex"
    ],
    "rules": {
      "no-console": "off",
      "require-jsdoc": "off",
      "no-warning-comments": "off",
      "max-len": "off",
      "no-undef": "off",
      "camelcase": "error",
      "consistent-return": "error",
      "curly": [
        "error",
        "multi-line",
        "consistent"
      ],
      "default-case": "error",
      "no-unsafe-regex/no-unsafe-regex": "error",
      "max-nested-callbacks": [
        "error",
        5
      ],
      "no-duplicate-imports": "error",
      "no-else-return": "error",
      "no-eval": "error",
      "no-extra-bind": "error",
      "no-extra-label": "error",
      "no-extra-parens": "error",
      "no-fallthrough": "error",
      "no-implicit-globals": "error",
      "no-implied-eval": "error",
      "no-invalid-this": "error",
      "no-mixed-requires": "error",
      "no-multi-spaces": "error",
      "no-native-reassign": "error",
      "no-new": "error",
      "no-new-func": "error",
      "no-new-wrappers": "error",
      "no-param-reassign": "error",
      "no-return-assign": "error",
      "no-self-compare": "error",
      "no-shadow-restricted-names": "error",
      "no-template-curly-in-string": "error",
      "no-undef-init": "error",
      "no-unsafe-negation": "error",
      "no-unused-expressions": "error",
      "no-useless-call": "error",
      "no-useless-computed-key": "error",
      "no-useless-concat": "error",
      "no-useless-constructor": "error",
      "no-useless-escape": "error",
      "no-useless-rename": "error",
      "no-var": "error",
      "no-void": "error",
      "no-with": "error",
      "object-shorthand": "error",
      "prefer-arrow-callback": "error",
      "prefer-const": "error",
      "semi": [
        "error",
        "never"
      ],
      "sort-imports": [
        "error",
        {}
      ],
      "strict": "error",
      "valid-jsdoc": "error"
    }
  }
}
