{
  "opts": {
    "destination": "./dist/docs/",
    "pedantic": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "includePattern": ".+\\.js(doc)?$"
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "layoutFile": "./jsdoc/layout.tmpl",
      "staticFiles": {
        "include": [
          "./jsdoc/static"
        ]
      }
    },
    "monospaceLinks": false
  }
}
